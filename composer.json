{
    "name": "origami/html",
    "description": "Swap-in package to replace laravelcollective/html with spatie/html",
    "license": "MIT",
    "authors": [
        {
            "name": "Papertank Limited",
            "homepage": "https://papertank.com",
            "email": "hello@papertank.com"
        },
        {
            "name": "David Rushton",
            "email": "david@papertank.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "spatie/laravel-html": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Origami\\Html\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Origami\\Html\\HtmlServiceProvider"
            ],
            "aliases": {
                "Html": "Origami\\Html\\Facades\\Html",
                "Form": "Origami\\Html\\Facades\\Form"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
